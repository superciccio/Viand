use router
use Home from "./Home.viand"
use About from "./About.viand"
use Dashboard from "./Dashboard.viand"
use Persistence from "./Persistence.viand"
use NotFound from "./NotFound.viand"
use Shell from "./Shell.viand"

component App:
    view:
        Shell:
            match router.path:
                case "/": Home
                case "/about": About
                case "/dashboard": Dashboard
                case "/persistence": Persistence
                default: NotFound